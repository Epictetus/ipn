= form_for @paypal_payment_notification do |f|
  -if @paypal_payment_notification.errors.any?
    #error_explanation
      %h2= "#{pluralize(@paypal_payment_notification.errors.count, "error")} prohibited this paypal_payment_notification from being saved:"
      %ul
        - @paypal_payment_notification.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :params
    = f.text_area :params
  .field
    = f.label :status
    = f.text_field :status
  .field
    = f.label :paypal_transaction_id
    = f.text_field :paypal_transaction_id
  .field
    = f.label :invoice
    = f.text_field :invoice
  .field
    = f.label :raw_post
    = f.text_area :raw_post
  .actions
    = f.submit 'Save'
